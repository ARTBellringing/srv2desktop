#tag WindowCode
	#tag Event
		Sub Dismissed()
		  // reset the session time out back to default
		  session.UserTimeout = Session.kIdleTimeout
		End Sub
	#tag EndEvent

	#tag Event
		Sub Opening()
		  //stop the session timeout so the user has to react to the dialog...
		  Session.UserTimeout = 0
		End Sub
	#tag EndEvent


#tag EndWindowCode

#tag Events btnOK
	#tag Event
		Sub Pressed()
		  self.Close
		  Module1.appClose
		  
		End Sub
	#tag EndEvent
#tag EndEvents
